*{box-sizing:border-box}body{margin:0;padding:0;letter-spacing:0}h1,h2,h3,h4,p,ul,li{margin:0;padding:0}h1{font:var(--main-heading-font-700);font-size:3.5rem;line-height:1.125}h2{font:var(--main-heading-font-600);font-size:2rem;line-height:1.25}h3{font:var(--main-heading-font-600);font-size:1.75rem;line-height:1.25}h4{font:var(--main-heading-font-700);font-size:1.5rem;line-height:1.25}p.eyebrow{font:var(--main-body-font-700);font-size:1.5rem;line-height:1.5;letter-spacing:2px}p,a{font:var(--main-body-font-400);font-size:1rem;line-height:1.5}label{font:var(--main-body-font-400);font-size:.75rem;line-height:2}button{font:var(--main-heading-font-600);font-size:.875rem;line-height:1.5}.elevation-default{box-shadow:0 1px 6px rgba(27,27,29,.15)}.elevation-medium{box-shadow:0 6px 12px rgba(27,27,29,.15)}.elevation-large{box-shadow:0 12px 20px rgba(27,27,29,.15)}